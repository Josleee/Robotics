#JSGF v1.0;

grammar vcc;

public <all> = <command> <locations> <find_object>;

<command> = [Alice] goto;

<locations> = <location> | <location> [and|or] <location>;

<location> = table one | table two;

<find_object> = and find [the] <object>;

<object> = ( chips | coca-cola | yellow container | salt | tomato soup | fanta | aadrink | chicken soup | shampoo | eraserbox | all objects );



